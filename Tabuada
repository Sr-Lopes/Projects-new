q = ('CALCULADORA DE TABUADA')
f = ('FIM DO CÓDIGO')
print(f'{q:-^40}')
s = int(input('Digite um número: '))
print(f'Essa é a tabuada de {s}')
print(f'{s} x 1 = ',s * 1)
print(f'{s} x 2 = ',s * 2)
print(f'{s} x 3 = ',s * 3)
print(f'{s} x 4 = ',s * 4)
print(f'{s} x 5 = ',s * 5)
print(f'{s} x 6 = ',s * 6)
print(f'{s} x 7 = ',s * 7)
print(f'{s} x 8 = ',s * 8)
print(f'{s} x 9 = ',s * 9)
print(f'{s} x 10 = ',s * 10)
print(f'{f:-^40}')
